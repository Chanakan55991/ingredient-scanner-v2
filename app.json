{
  "expo": {
    "name": "ingredient-scanner-v2",
    "slug": "ingredient-scanner-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "ingsc",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.chanakancloud.ingredientscanner",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to camera for scanning ingredients"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "net.chanakancloud.ingredientscanner",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to camera for scanning ingredients",
          "disableFrameProcessors": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b4ee2c9b-3cdc-4f84-a414-dcc50016409f"
      }
    }
  }
}
